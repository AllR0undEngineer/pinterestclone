<%include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-800 text-white ">
      <div class="cards flex items-center gap-5 ">
       <% posts.forEach(function(post){ %>
        <div class="card ml-4 px-8 py-4 w-42">
            <div class="w-full h-32 bg-white rounded-lg overflow-hidden">
                <img class="w-full h-full object-cover" src="/images/uploads/<%=post.image%>" alt="img">
               </div> 
               <h3>Title-<%= post.title %></h3>
            <h3> description-<%= post.description %></h3>
            <h4 class="text-xs opacity-50">@<%=post.user.username%></h4>
            </div>
       <% }) %>
      </div>
       
</div>
<%include ./partials/footer.ejs %>